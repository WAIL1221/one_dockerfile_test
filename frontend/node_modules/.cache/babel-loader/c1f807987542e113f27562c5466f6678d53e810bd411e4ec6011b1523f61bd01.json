{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nexport default {\n  name: 'StudentEnsat',\n  data() {\n    return {\n      id: 1,\n      indice: -1,\n      etudiants: []\n    };\n  },\n  methods: {\n    save: function () {\n      if (this.indice === -1) {\n        this.etudiants.push({\n          id: this.id++,\n          nom: this.nom,\n          prenom: this.prenom,\n          note: this.note\n        });\n        alert(\"Insert\");\n      } else alert(\"Update\");\n      this.nom = this.prenom = this.note = \"\";\n    },\n    retirer: function () {\n      this.etudiants.pop();\n    }\n  }\n};","map":{"version":3,"names":["name","data","id","indice","etudiants","methods","save","push","nom","prenom","note","alert","retirer","pop"],"sources":["C:\\app\\Frontend\\src\\components\\Etudiant.vue"],"sourcesContent":["<script >\r\nexport default {\r\n  name: 'StudentEnsat',\r\n  data(){\r\n    return{\r\n      id:1,\r\n      indice:-1,\r\n      etudiants:[]\r\n    }\r\n  },\r\n  methods:{\r\n    save:function (){\r\n      if(this.indice===-1){\r\n        this.etudiants.push({id:this.id++,nom:this.nom,prenom:this.prenom,note:this.note});\r\n        alert(\"Insert\");\r\n      }\r\n      else alert(\"Update\")\r\n      this.nom=this.prenom=this.note=\"\";\r\n    },\r\n    retirer:function (){\r\n      this.etudiants.pop();\r\n    }\r\n  }\r\n}\r\n</script>\r\n\r\n<template>\r\n  <div>\r\n    <h1>Gestion des étudiants</h1>\r\n    <form v-on:submit.prevent=\"save\">\r\n      <div class=\"form-group\">\r\n        <label for=\"Nom\">Nom</label>\r\n        <input type=\"text\" id=\"Nom\" v-model=\"nom\" class=\"form-control\" aria-describedby=\"emailHelp\" placeholder=\"Entrer le nom de l'étudiant\">\r\n      </div>\r\n      <div class=\"form-group\">\r\n        <label for=\"Prénom\">Prénom</label>\r\n        <input type=\"text\" id=\"Prénom\" v-model=\"prenom\" class=\"form-control\" aria-describedby=\"emailHelp\" placeholder=\"Entrer le prénom  étudiant\">\r\n      </div>\r\n      <div class=\"form-group\">\r\n        <label for=\"Note\">Note</label>\r\n        <input type=\"text\" id=\"Note\" v-model=\"note\" class=\"form-control\" aria-describedby=\"emailHelp\" placeholder=\"Entrer la note de l'étudiant\">\r\n      </div>\r\n      <br>\r\n      <button type=\"submit\" class=\"btn btn-primary\">Save</button>\r\n    </form>\r\n  </div>\r\n  <br><br>\r\n  <table class=\"table table-striped table-dark\">\r\n    <thead>\r\n    <tr>\r\n      <th scope=\"col\">ID</th>\r\n      <th scope=\"col\">Nom</th>\r\n      <th scope=\"col\">Prénom</th>\r\n      <th scope=\"col\">Note</th>\r\n      <th scope=\"col\"><a href=\"\" @click.prevent=\"retirer\">Retirer</a></th>\r\n    </tr>\r\n    </thead>\r\n    <tbody>\r\n    <tr v-for=\"E in etudiants\" :key=\"E\">\r\n      <th scope=\"row\">{{ E.id }}</th>\r\n      <td>{{ E.nom }}</td>\r\n      <td>{{ E.prenom}}</td>\r\n      <td>{{ E.note}}</td>\r\n      <td><button type=\"button\" class=\"btn btn-primary\">Edit</button> / \r\n        <button type=\"button\" class=\"btn btn-danger\">Delete</button></td>\r\n    </tr>\r\n    </tbody>\r\n  </table>\r\n\r\n\r\n</template>\r\n\r\n<style scoped>\r\n\r\n</style>"],"mappings":";AACA,eAAe;EACbA,IAAI,EAAE,cAAc;EACpBC,IAAIA,CAAA,EAAE;IACJ,OAAM;MACJC,EAAE,EAAC,CAAC;MACJC,MAAM,EAAC,CAAC,CAAC;MACTC,SAAS,EAAC;IACZ;EACF,CAAC;EACDC,OAAO,EAAC;IACNC,IAAI,EAAC,SAAAA,CAAA,EAAW;MACd,IAAG,IAAI,CAACH,MAAM,KAAG,CAAC,CAAC,EAAC;QAClB,IAAI,CAACC,SAAS,CAACG,IAAI,CAAC;UAACL,EAAE,EAAC,IAAI,CAACA,EAAE,EAAE;UAACM,GAAG,EAAC,IAAI,CAACA,GAAG;UAACC,MAAM,EAAC,IAAI,CAACA,MAAM;UAACC,IAAI,EAAC,IAAI,CAACA;QAAI,CAAC,CAAC;QAClFC,KAAK,CAAC,QAAQ,CAAC;MACjB,OACKA,KAAK,CAAC,QAAQ;MACnB,IAAI,CAACH,GAAG,GAAC,IAAI,CAACC,MAAM,GAAC,IAAI,CAACC,IAAI,GAAC,EAAE;IACnC,CAAC;IACDE,OAAO,EAAC,SAAAA,CAAA,EAAW;MACjB,IAAI,CAACR,SAAS,CAACS,GAAG,CAAC,CAAC;IACtB;EACF;AACF","ignoreList":[]},"metadata":{},"sourceType":"module","externalDependencies":[]}